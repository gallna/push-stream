        "vlucas/phpdotenv": "^2.2",
        "gallna/stream": "*",
        "slim/slim": "^3.3"
    },
    "authors": [
        {
            "name": "Tomasz Jonik",
            "email": "tom.jonik@hotmail.com"
        }
    ],
